<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:motionInterpolator="cubic(0.31,0.98,0.36,0)"
        motion:autoTransition="animateToEnd"
        >
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe motion:maxAcceleration="0.1"
            motion:maxVelocity="0.3" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/backgroundLayout"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="10"
            android:scaleY="10" />
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            android:layout_height="150dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="0"
            android:scaleY="0"
            motion:transitionEasing="cubic(1,0,0.47,0)"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imageView"
            android:scaleX="0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            android:layout_height="150dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleY="0" />
        <Constraint
            android:id="@+id/constraintLayout"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_editor_absoluteY="99dp"
            motion:layout_editor_absoluteX="247dp"
            android:scaleX="0" />
        <Constraint
            android:id="@+id/constraintLayout"
            android:scaleX="0"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_editor_absoluteY="99dp"
            motion:layout_editor_absoluteX="247dp"
            android:scaleY="0" />
        <Constraint
            android:id="@+id/constraintLayout2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_editor_absoluteY="134dp"
            motion:layout_editor_absoluteX="33dp"
            android:scaleX="0" />
        <Constraint
            android:id="@+id/constraintLayout2"
            android:scaleX="0"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_editor_absoluteY="134dp"
            motion:layout_editor_absoluteX="33dp"
            android:scaleY="0" />
        <Constraint
            android:id="@+id/constraintLayout3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_editor_absoluteY="517dp"
            motion:layout_editor_absoluteX="58dp"
            android:scaleX="0"
            android:scaleY="0" />
        <Constraint
            android:id="@+id/constraintLayout4"
            android:layout_width="80dp"
            android:layout_height="80dp"
            motion:layout_editor_absoluteY="635dp"
            motion:layout_editor_absoluteX="296dp"
            android:scaleX="0"
            android:scaleY="0" />
        <Constraint
            android:id="@+id/constraintLayout4"
            android:scaleX="0"
            android:layout_width="80dp"
            android:layout_height="80dp"
            motion:layout_editor_absoluteY="635dp"
            motion:layout_editor_absoluteX="296dp"
            android:scaleY="0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="180dp"
            android:layout_height="150dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="2"
            android:scaleY="2"
            motion:transitionEasing="cubic(1,0,0.61,0)" />
        <Constraint
            android:id="@+id/backgroundLayout"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="200dp"
            android:layout_height="200dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:scaleX="0"
            android:scaleY="0" />
        <Constraint
            android:id="@+id/constraintLayout"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_editor_absoluteY="99dp"
            motion:layout_editor_absoluteX="247dp"
            android:scaleX="1" />
        <Constraint
            android:id="@+id/constraintLayout"
            android:scaleX="1"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_editor_absoluteY="99dp"
            motion:layout_editor_absoluteX="247dp"
            android:scaleY="1" />
        <Constraint
            android:id="@+id/constraintLayout2"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_editor_absoluteY="134dp"
            motion:layout_editor_absoluteX="33dp"
            android:scaleX="1"
            android:scaleY="1" />
        <Constraint
            android:id="@+id/constraintLayout2"
            android:scaleX="1"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_editor_absoluteY="134dp"
            motion:layout_editor_absoluteX="33dp"
            android:scaleY="1" />
        <Constraint
            android:id="@+id/constraintLayout3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_editor_absoluteY="517dp"
            motion:layout_editor_absoluteX="58dp"
            android:scaleX="1"
            android:scaleY="1" />
        <Constraint
            android:id="@+id/constraintLayout4"
            android:layout_width="80dp"
            android:layout_height="80dp"
            motion:layout_editor_absoluteY="635dp"
            motion:layout_editor_absoluteX="296dp"
            android:scaleX="1"
            android:scaleY="1" />
    </ConstraintSet>
</MotionScene>